@model IEnumerable<Mstudent>
@{
    ViewData["Title"] = "Home Page";
}
<div class="=container">
    <table class="table table-striped">
        <thead>
        <th>Name</th>
        <th>Age</th>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>  @item.Name </td>
                    <td>  @item.Age </td>
                </tr>
            }
        </tbody>
    </table>
</div>



